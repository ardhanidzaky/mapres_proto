{
    "builds": [{
        "src": "mapres_prototype/wsgi.py",
        "use": "@vercel/python",
        "config": { 
            "maxLambdaSize": "15mb",
            "runtime": "python3.9"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mapres_prototype/wsgi.py"
        }
    ]
}